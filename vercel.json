{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build-storybook",
    "devCommand": "npm run storybook",
    "installCommand": "npm install",
    "framework": null,
    "outputDirectory": "./storybook-static",
    "redirects": [
    {
      "source": "/storybook",
      "destination": "/storybook/"
    }
  ],
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "github": {
    "silent": true
  }
  }